<p id="notice"><%= notice %></p>

<br>
<h2>Your reports</h2>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
      <% @subject.reports.each do |report|  %>
      <tr>
        <td><%=report.name%></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<br>
<h2>Your regisering classes</h2>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= link_to subject.name, subject_path(subject.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
